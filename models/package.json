{
  "name": "@simulex/models",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "models",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "models/dist",
          "tsConfig": "models/tsconfig.lib.json",
          "packageJson": "models/package.json",
          "main": "models/src/index.ts",
          "stripLeadingPaths": true,
          "sourceMap": true
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}