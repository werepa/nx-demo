    "rootDir": "../models/src",
    "outDir": "../models/dist",
    "tsBuildInfoFile": "../models/dist/tsconfig.lib.tsbuildinfo",
    "emitDeclarationOnly": false,
    "types": [
      "node"
    ]
  },
  "include": [
    "../models/src/**/*.ts"
